{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "checkJs": false
  },
  "exclude": [
    "node_modules",
    "vendor",
    "**/*.blade.php",
    "**/*.blade.php",
    "laravel/resources/views/**",
    "laravel/storage/**",
    "laravel/bootstrap/cache/**",
    "**/resources/views/**"
  ],
  "include": []
}

